{
  "name": "zypher-project",
  "version": "1.0.0",
  "description": "Zypher AI agent integration project using Deno",
  "exports": "./main.ts",
  "tasks": {
    "dev": "deno run --allow-net --allow-read --allow-write --allow-env main.ts",
    "start": "deno run --allow-net --allow-read --allow-write --allow-env main.ts"
  },
  "imports": {
    "@corespeed/zypher": "jsr:@corespeed/zypher@^0.2.1",
    "@std/": "https://deno.land/std@0.224.0/",
    "rxjs-for-await": "npm:rxjs-for-await@^1.0.0",
    "openai": "npm:openai@4.28.0"
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "lib": ["deno.window"],
    "strict": true
  }
}